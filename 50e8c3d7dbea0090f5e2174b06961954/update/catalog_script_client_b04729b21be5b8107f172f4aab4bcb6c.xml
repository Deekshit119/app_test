        <name>HVS - Display Manager Info</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

	var ga = new GlideAjax('global.HVSUserContractUtils');
	ga.addParam('sysparm_name', 'getContractInfos');
	ga.addParam('sysparm_contract_id', newValue);
	ga.getXML(parseResponse);

	function parseResponse(response) {
		var answer = JSON.parse(response.responseXML.documentElement.getAttribute("answer"));

		if(answer && answer.manager){
			g_form.hideFieldMsg('contract', true);
			g_form.showFieldMsg('contract', getMessage('Manager') + ' : ' + answer.manager);
		}
	}
	
}]]></script>
