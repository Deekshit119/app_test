        <script><![CDATA[/*** --- Do not modify or remove this script !! --- Automatically added by "Add Predefined script" business rule. 
Forces population of this record producer sys_id into the target record***/
var rpId = "ff844a95db3ec090011e177e16961952";
current.u_cat_item = rpId;
/***------------------ ***/
if(!gs.isMobile()) {
	producer.portal_redirect = "exphvs";
}

current.opened_by = gs.getUserID();
current.contract = producer.contract;

// Générer les ligens de dépenses
var assignment_group = gs.getProperty('x_hdvi2_expense.salary_group.id');
current.assignment_group = assignment_group;
current.short_description = producer.short_description;
current.status = 1;
current.total_requested_amount = producer.sr_requested_amount;

var gr_expLine = new GlideRecord("x_hdvi2_expense_line");
gr_expLine.initialize();
gr_expLine.expense = current.sys_id;
gr_expLine.status = 0;
if (producer.sr_doublon == 'Yes') gr_expLine.duplicated = true;
else gr_expLine.duplicated = false;
gr_expLine.currency = producer.sr_currency;
gr_expLine.location_change_reason = producer.sr_new_location_from_reason;
gr_expLine.date_expense = producer.sr_date_expense;
gr_expLine.location = producer.sr_location_from;
gr_expLine.travelkm = producer.sr_location_to;
gr_expLine.one_way = producer.sr_one_way;
gr_expLine.other_location = producer.sr_location_other;
gr_expLine.other_location_to = producer.sr_location;
gr_expLine.requested_amount = parseFloat(producer.sr_requested_amount || '0');
gr_expLine.requested_value = parseFloat(producer.sr_requested_value || '0');
//gr_expLine.refunded_amount =  producer.sr_requested_amount || '0';
//gr_expLine.refunded_value = producer.sr_requested_value || '0';
gr_expLine.rubric = producer.sr_rubrique_loga;
gr_expLine.description = producer.short_description;
// for storing selected category for rubrc = Divers
var sDivers = producer.sr_miscellaneous_account_code.getDisplayValue() + '';
if (sDivers != '')
    gr_expLine.miscellaneous_account_code = producer.sr_miscellaneous_account_code.getDisplayValue() + " | " + producer.sr_miscellaneous_account_code;
else
    gr_expLine.miscellaneous_account_code = '';
gr_expLine.insert();]]></script>
